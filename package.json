{
  "name": "prisma-query-inspector",
  "version": "1.0.1",
  "description": "Prisma 2 tool to inspect all queries going to the database, formatted and with all params if any.",
  "main": "dist/server/index.js",
  "repository": "https://github.com/omar-dulaimi/prisma-query-inspector",
  "scripts": {
    "start": "NODE_ENV=production node dist/server/app.js",
    "dev": "npm run build-client && ts-node-dev --respawn --transpile-only server/app.ts",
    "build": "npm run build-client && npm run build-server",
    "build-client": "cd client && npm run build",
    "build-server": "cd server && tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Omar Dulaimi",
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-query-inspector"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "axios": "^0.26.1",
    "boxen": "^4.0.0",
    "chalk": "^4.1.0",
    "express": "^4.17.3",
    "socket.io": "^4.4.1",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "ts-node-dev": "^1.1.8"
  },
  "files": [
    "dist",
    "!**/*.json"
  ]
}
